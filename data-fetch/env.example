# Data-Fetch Framework Environment Variables
# Copy this file to .env and fill in your values

# Required for dynamic URL support (LLM-powered detection)
OPENAI_API_KEY=sk-your-openai-key-here

# Optional - for fallback sources (only if using paid tiers)
COINGECKO_API_KEY=your-coingecko-key-here
# CoinGecko Pro API (set to true/1/yes if using Pro tier)
COINGECKO_USE_PRO=false
# CoinDesk API key (formerly CryptoCompare)
COINDESK_API_KEY=your-coindesk-key-here
# Legacy CryptoCompare API key (deprecated, use COINDESK_API_KEY)
CRYPTOCOMPARE_API_KEY=your-cryptocompare-key-here
# Alpha Vantage API key (free tier: 5 calls/minute, 500/day)
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key-here

# Dune Analytics API key (for blockchain analytics queries)
# Get your API key from: https://dune.com/settings/api
DUNE_API_KEY=your-dune-api-key-here

# CoinGlass API key (optional - browser scraping is used instead of paid API)
# COINGLASS_API_KEY=your-coinglass-key-here

# CME Group API key (optional - requires paid subscription)
# CME_API_KEY=your-cme-api-key-here

# Optional - Custom user agent for scraping
USER_AGENT=DataFetchBot/1.0 (Educational purposes)

# Optional - Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional - Authentication and session management
COOKIE_STORAGE_PATH=~/.data-fetch/cookies
SESSION_TIMEOUT=3600
AUTH_RETRY_ATTEMPTS=2

# Optional - Proxy server (e.g., http://proxy.example.com:8080)
PROXY_SERVER=

# Optional - Stealth mode (true/false)
USE_STEALTH_MODE=true

